<%= content_for :js do %>
  <%= javascript_pack_tag 'shows/index', 'data-turbolinks-track': 'reload' %>
<% end %>
<div id="shows" class="container">
<div class="row mt-5">
    <% @shows.each do |show| %>
     <a href="<%= show_url(show.id) %>">
        <show :show="<%= show.to_json %>"></show>
     </a>
    <% end %>
</div>
</div>
